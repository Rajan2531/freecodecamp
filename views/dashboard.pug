
mixin courseRow(course,duration) 
    .courseDetails 
        img(src=`/img/apple logo.png` width="40px" )
        p  #{course}  (#{duration})

doctype html
html
    head
        meta(charset='UTF-8')
        meta(http-equiv='X-UA-Compatible' content='IE=edge')
        meta(name='viewport' content='width=device-width, initial-scale=1.0')
        title Document
        link(rel='stylesheet' type ="text/css" href='/css/dashboardStyle.css')
        
        //- //link( href="https://fonts.googleapis.com/css2?family=Bree+Serif&family=Gloria+Hallelujah&family=Lato:wght@100&family=Patua+One&display=swap" rel="stylesheet")

    
        include _header
        .content1 
            h3 Welcome to freeCodeCamp.org
            p "I have not failed. I've just found 10,000 ways 
            p that wont work."
            p - Thomas A. Edision
            each course in courses
                +courseRow(course.title,course.duration)

